{
  "name": "boj-get",
  "displayName": "BOJ Get",
  "private": true,
  "description": "백준의 문제를 쉽게 가져오고, 테스트합니다.",
  "publisher": "niceb5y",
  "license": "GLWTPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/niceb5y/boj-get.git"
  },
  "version": "1.0.0",
  "bugs": {
    "url": "http://github.com/niceb5y/boj-get/issues",
    "email": "niceb5y@gmail.com"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.newProblem"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.newProblem",
        "title": "백준에서 새로운 문제 가져오기"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/mocha": "^2.2.42",
    "@types/node": "^11.11.6",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "vsce": "^1.58.0"
  }
}
